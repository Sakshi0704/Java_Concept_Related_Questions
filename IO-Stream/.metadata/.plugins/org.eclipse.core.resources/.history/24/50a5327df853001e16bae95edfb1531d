package com.filereader;

import java.io.*;

public class Main {
public static void main(String[] args) {
		try {
			File file = new File("abc.txt");
			if (file.createNewFile()) {
				System.out.println("New File is created!");
			} else {
				System.out.println("File already exists.");
			}
		}catch (IOException e) {
				e.printStackTrace();
			}
		}
}